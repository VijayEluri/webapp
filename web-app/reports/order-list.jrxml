	<queryString language="SQL">
		<![CDATA[SELECT
    shout.experiment_id,
    timeslot.start_date_time,
    trading_agent.name,
    shout.date_created,
    shout.date_closed,
    shout.quantity,
    shout.limit_price,
    shout.price_alloc,
    shout.quantity_alloc,
    shout.shout_status,
    shout.shout_type,
    shout.comment
FROM
    shout
INNER JOIN timeslot
ON
    (
        shout.timeslot_id = timeslot.id
    )
INNER JOIN trading_agent
ON
    (
        shout.trading_agent_id = trading_agent.id
    )
where shout.experiment_id=$P{experiment.id}
order by trading_agent.name, shout.id;]]>
