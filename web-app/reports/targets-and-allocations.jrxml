	<parameter name="experiment.id" class="java.lang.String">
		<defaultValueExpression><![CDATA["Please specify an experiment!"]]></defaultValueExpression>
	</parameter>
	<queryString language="SQL">
		<![CDATA[SELECT
    se.experiment_id   AS experiment_id,
    ts.start_date_time AS start_date_time,
    ta.name            AS name,
    (SELECT s.power_target FROM stock_entry s WHERE ((s.experiment_id = se.experiment_id) AND (s.timeslot_id = se.timeslot_id) AND (s.trading_agent_id = se.trading_agent_id) AND (s.stock_type = "Consumption") AND (s.description = 'Set initial power target'))) AS initial_consumption_target,
    (SELECT (-1 * s.power_target) FROM stock_entry s WHERE ((s.experiment_id = se.experiment_id) AND (s.timeslot_id = se.timeslot_id) AND (s.trading_agent_id = se.trading_agent_id) AND (s.stock_type = "Generation") AND (s.description = 'Set initial power target'))) AS initial_generation_target,
    (SELECT (-1 * SUM(s.power_target)) FROM stock_entry s WHERE ((s.experiment_id = se.experiment_id) AND (s.timeslot_id = se.timeslot_id) AND (s.trading_agent_id = se.trading_agent_id) AND (s.stock_type = "Generation"))) AS final_generation_target,
    (SELECT SUM(s.power_target) FROM stock_entry s WHERE ((s.experiment_id = se.experiment_id) AND (s.timeslot_id = se.timeslot_id) AND (s.trading_agent_id = se.trading_agent_id) AND (s.stock_type = "Consumption"))) AS final_consumption_target,
    (SELECT (-1 * SUM(s.power_allocated)) FROM stock_entry s WHERE ((s.experiment_id = se.experiment_id) AND (s.timeslot_id = se.timeslot_id) AND (s.trading_agent_id = se.trading_agent_id) AND (s.stock_type = "Generation"))) AS final_generation_allocated,
    (SELECT SUM(s.power_allocated) FROM stock_entry s WHERE ((s.experiment_id = se.experiment_id) AND (s.timeslot_id = se.timeslot_id) AND (s.trading_agent_id = se.trading_agent_id) AND (s.stock_type = "Consumption"))) AS final_consumption_allocated
FROM
    ((stock_entry se
JOIN timeslot ts
ON ((se.timeslot_id = ts.id)))
JOIN trading_agent ta
ON ((se.trading_agent_id = ta.id)))
WHERE se.experiment_id=$P{experiment.id}
GROUP BY
    se.experiment_id,
    ts.start_date_time,
    ta.name,
    initial_consumption_target,
    initial_generation_target,
    final_generation_target,
    final_consumption_target,
    final_generation_allocated,
    final_consumption_allocated]]>
	</queryString>
